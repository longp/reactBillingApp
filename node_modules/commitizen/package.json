{
  "name": "commitizen",
  "version": "2.8.2",
  "description": "Git commit, but play nice with conventions.",
  "main": "src/index.js",
  "scripts": {
    "check-coverage": "nyc check-coverage --statements 80 --branches 80 --functions 80 --lines 80 ",
    "commit": "node bin/git-cz",
    "build": "babel src --out-dir dist",
    "build:watch": "babel --watch src --out-dir dist",
    "prepublish": "npm run build",
    "report-coverage": "nyc report --reporter=lcov | codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "npm run test:watch",
    "test": "nyc --require babel-core/register _mocha -- test/tests/index.js",
    "test:watch": "nodemon -q --ignore test/.tmp/ --ignore test/artifacts/ --ignore coverage/ --exec \"npm run test\" --",
    "test:windows": "node ./test/tools/trigger-appveyor-tests.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  },
  "homepage": "https://github.com/commitizen/cz-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commitizen/cz-cli.git"
  },
  "keywords": [
    "commit",
    "pretty",
    "format",
    "conventional changelog",
    "commitizen"
  ],
  "bugs": {
    "url": "https://github.com/commitizen/cz-cli/issues"
  },
  "bin": {
    "git-cz": "./bin/git-cz",
    "commitizen": "./bin/commitizen"
  },
  "author": {
    "name": "Jim Cummins",
    "email": "jimthedev@gmail.com",
    "url": "https://github.com/jimthedev"
  },
  "license": "MIT",
  "devDependencies": {
    "axios": "0.12.0",
    "babel-cli": "6.6.5",
    "babel-core": "6.9.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-2": "6.5.0",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "ghooks": "1.2.1",
    "mocha": "2.5.3",
    "node-uuid": "1.4.7",
    "nodemon": "1.9.1",
    "nyc": "6.4.4",
    "proxyquire": "^1.7.4",
    "rimraf": "2.5.2",
    "semantic-release": "^4.3.5",
    "semver": "5.1.0",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "cz-conventional-changelog": "1.1.6",
    "dedent": "0.6.0",
    "detect-indent": "4.0.0",
    "find-node-modules": "1.0.1",
    "find-root": "^1.0.0",
    "glob": "7.0.3",
    "gulp": "3.9.1",
    "gulp-git": "1.7.2",
    "home-or-tmp": "2.0.0",
    "inquirer": "1.0.3",
    "lodash": "4.11.1",
    "minimist": "1.2.0",
    "shelljs": "0.5.3",
    "strip-json-comments": "2.0.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "nyc": {
    "exclude": [
      "src/cli",
      "test"
    ]
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 0.12"
  },
